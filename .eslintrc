{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": [
    "react",
    "jsx-a11y",
    "import",
    "html"
  ],
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
  },
  "rules": {
        "jsx-a11y/label-has-for": 0,
    },
  "settings" : {
    "import/core-modules": "config.js",
    "import/resolver": {
      "webpack": {
        "config": "./config/webpack.BASE.config.js",
      }
    }
  },
}
